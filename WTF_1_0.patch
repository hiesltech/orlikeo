 \"\";\n\t\twhile ($field = pg_fetch_assoc($result)) {\n\t\t    $fields[] = $field[\"column_name\"];\n\t\t}\n\t\treturn array('fields' => $fields, 'key' => $id );\n\t}\n}\n?>
